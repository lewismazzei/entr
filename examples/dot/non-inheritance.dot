    outputorder="edgesfirst";

    node [shape=plaintext] student
    [label=<
        <TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0" PORT="port">
            <TR><TD BGCOLOR="#C7EAFB" CELLPADDING="4">
                <I>student</I>
            </TD></TR>
            <TR><TD
                BALIGN="LEFT"
                ALIGN="LEFT"
                WIDTH="80"
                HEIGHT="50"
                CELLPADDING="6"
